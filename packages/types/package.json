{
    "name": "@tenex/types",
    "version": "1.0.0",
    "description": "Shared TypeScript type definitions for TENEX",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./agents": {
            "types": "./dist/agents/index.d.ts",
            "import": "./dist/agents/index.js"
        },
        "./llm": {
            "types": "./dist/llm/index.d.ts",
            "import": "./dist/llm/index.js"
        },
        "./tools": {
            "types": "./dist/tools/index.d.ts",
            "import": "./dist/tools/index.js"
        },
        "./projects": {
            "types": "./dist/projects/index.d.ts",
            "import": "./dist/projects/index.js"
        },
        "./events": {
            "types": "./dist/events/index.d.ts",
            "import": "./dist/events/index.js"
        },
        "./conversations": {
            "types": "./dist/conversations/index.d.ts",
            "import": "./dist/conversations/index.js"
        },
        "./ui": {
            "types": "./dist/ui/index.d.ts",
            "import": "./dist/ui/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js"
        },
        "./telemetry": {
            "types": "./dist/telemetry/index.d.ts",
            "import": "./dist/telemetry/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "clean": "rm -rf dist",
        "type-check": "tsc --noEmit"
    },
    "keywords": ["tenex", "types", "typescript"],
    "author": "TENEX",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^22.10.6",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "@nostr-dev-kit/ndk": "*"
    },
    "files": ["dist"]
}
