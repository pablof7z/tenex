{
    "name": "@tenex/cli",
    "version": "0.1.0",
    "description": "TENEX Command Line Interface",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "tenex": "./bin/tenex.js"
    },
    "files": ["dist", "bin"],
    "scripts": {
        "start": "bun run ./bin/tenex.ts",
        "build": "bun scripts/build-bundled.js",
        "build:simple": "bun scripts/build-simple.js",
