        "@types/inquirer": "^9.0.8",
        "chalk": "^5.4.1",
        "commander": "^13.1.0",
        "fs-extra": "^11.2.0",
        "inquirer": "^12.6.3",
        "js-base64": "^3.7.7",
        "jwt-js-decode": "^1.9.0",
        "nostr-tools": "^2.15.0"
    },
    "devDependencies": {
        "@types/uuid": "^10.0.0",
